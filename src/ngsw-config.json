{
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/favicon.ico",
            "/index.html",
            "/*.css",
            "/*.js"
          ]
        }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
          "files": [
            "/assets/**",
            "/*.(png|jpg)"
          ]
        }
      }
    ],
    "dataGroups": [
      {
        "name": "api",
        "urls": [
          "https://competex.schnykjaer.com:22114/api/**"  
        ],
        "cacheConfig": {
          "strategy": "freshness",
          "maxSize": 100,
          "maxAge": "1h",
          "timeout": "10s"
        }
      }
    ]
  }
  